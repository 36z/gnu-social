msgid "Search"
msgstr "Zoeken"

#, php-format
msgid "No users starting with %s"
msgstr "Er zijn geen gebruikers wiens naam begint met \"%s\""

#. TRANS: Empty list message for searching group directory.
msgid "No results."
msgstr "Geen resultaten."

